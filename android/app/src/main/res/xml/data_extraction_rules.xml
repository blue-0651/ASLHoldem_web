<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- 클라우드 백업에서 제외할 데이터 -->
        <exclude domain="database" />
        <exclude domain="sharedpref" path="sensitive_prefs.xml" />
        <exclude domain="external" path="cache/" />
    </cloud-backup>
    
    <device-transfer>
        <!-- 기기 이전 시 포함할 데이터 -->
        <include domain="sharedpref" path="user_preferences.xml" />
        <include domain="file" path="." />
    </device-transfer>
</data-extraction-rules> 